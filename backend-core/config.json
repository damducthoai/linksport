{
    "profile": "dev",
    "HttpVerticle": {
        "port": 8888,
        "allowUrl": "/api/v1",
        "allowMethod": "POST",
        "eventBinding": {
            "login": {
                "addr": "login_queue_dev",
                "schema": {
                    "id": "/Login",
                    "type": "object",
                    "properties": {
                        "service": {
                            "type": "string"
                        },
                        "user": {
                            "type": "string"
                        },
                        "password": {
                            "type": "string"
                        }
                    },
                    "required": ["user", "password"]
                }
            },
            "register": {
                "addr": "register_queue_dev",
                "schema": {
                    "id": "/Register",
                    "type": "object",
                    "properties": {
                        "service": {
                            "type": "string"
                        },
                        "data": {
                            "type": "object",
                            "properties":{
                                "user": {
                                    "type": "string"
                                },
                                "password": {
                                    "type": "string"
                                }
                            },
                            "required": ["user", "password"]
                        },
                        "required": ["service", "data"]
                    }
                }
            }
        }
    },
    "cassandra": {
        "username": "cassandra",
        "password": "cassandra",
        "host": "3.0.101.223",
        "port": 9042
    },
    "RabbitRpcClient": {
        "server": "amqp://linkedsport:linkedsportPssword@3.0.101.223:5672",
        "timeout": 1000
    }
}