    "@types/node": "^12.0.8",
    "@types/pg": "^7.4.14",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "amqplib": "^0.5.3",
    "backend-base": "file:../backend-base",
    "backend-rpc": "file:../backend-rpc",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^8.5.1",
